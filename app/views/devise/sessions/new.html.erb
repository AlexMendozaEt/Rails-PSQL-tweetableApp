<h2  class= "home_text">Sign in to your account</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="text_filds">
    <div class="field">
      <%= f.label :email, class: "text_labels_filds"  %><br />
      <%= f.email_field :email, autofocus: true, autocomplete: "off", class: "inputs_filds"   %>
    </div>

    <div class="field">
      <%= f.label :password, class: "text_labels_filds"  %><br />
      <%= f.password_field :password, autocomplete: "current-password", class: "inputs_filds"  %>
    </div>

    <div class="actions">
      <%= f.submit "Sing in", class: "buttom_regis" %>
    </div>
    <br><br><br>
    <p class="text-center">Donâ€™t have an account? <p>
    <div class="login_buttom">
    <%= render "devise/shared/links" %>
    </div>
  </div>
<% end %>


